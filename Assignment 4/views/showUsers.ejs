<%- include('includes/head.ejs')%>
<link rel="stylesheet" href="/css/users.css">
</head>
<body>
    <%-include('includes/navigation.ejs')%>
    <% if (users.length > 0) { %>
        <h1>Users :</h1>
        <div>
        <ul class="styled-list">
        <% for (let user of users) {%>
            <li><%= user.userName%></li>

        <% } %>
        </ul>
    </div>
    <% } else { %>
        <h1>No Users</h1>
        <% } %>
</body>
</html>